<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    tools:ignore="UnusedNavigation"
    app:startDestination="@id/listaCategoriaFragment">

    <fragment
        android:id="@+id/listaCategoriaFragment"
        android:name="com.gabrielmarrani.aula35.categoria.view.ListaCategoriaFragment"
        android:label="fragment_lista_categoria"
        tools:layout="@layout/fragment_lista_categoria" >
        <action
            android:id="@+id/action_listaCategoriaFragment_to_detalheFragment"
            app:destination="@id/detalheFragment" />
    </fragment>
    <fragment
        android:id="@+id/detalheFragment"
        android:name="com.gabrielmarrani.aula35.detalhecategoria.view.DetalheFragment"
        android:label="fragment_detalhe"
        tools:layout="@layout/fragment_detalhe" />
</navigation>